{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist"],
      "dependsOn": ["^build"]
    },
    "prettier-check": {
      "outputMode": "hash-only"
    },
    "prettier-fix": {
      "outputMode": "hash-only"
    },
    "test": {},
    "start": {
      "cache": false,
      "persistent": true
    }
  }
}
